    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const navLinks = document.querySelectorAll('.navbar a');
            const pageSections = document.querySelectorAll('.page-section');
            
            // Function to show a specific page
            function showPage(pageId) {
                // Hide all pages
                pageSections.forEach(section => {
                    section.classList.remove('active');
                });
                
                // Remove active class from all nav links
                navLinks.forEach(link => {
                    link.classList.remove('active');
                });
                
                // Show the requested page
                const targetPage = document.getElementById(pageId);
                if (targetPage) {
                    targetPage.classList.add('active');
                    
                    // Add active class to the corresponding nav link
                    document.querySelector(`.navbar a[href="#${pageId}"]`).classList.add('active');
                    
                    // Animate elements on the page
                    setTimeout(() => {
                        animatePageElements(targetPage);
                    }, 100);
                }
            }
            
            // Function to animate elements on a page
            function animatePageElements(page) {
                const elements = page.querySelectorAll('.card, table, .commitment-item, .certification-box, .contact-info');
                elements.forEach((element, index) => {
                    setTimeout(() => {
                        element.classList.add('animate-in');
                    }, 100 + (index * 150));
                });
            }
            
            // Add click event listeners to nav links
            navLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    const pageId = this.getAttribute('href').substring(1);
                    showPage(pageId);
                    
                    // Scroll to top of the page
                    window.scrollTo({ top: 0, behavior: 'smooth' });
                });
            });
            
            // Show the first page on load
            showPage('placement-overview');
            
            // Animate elements on the initial page
            animatePageElements(document.querySelector('.page-section.active'));
        });
    </script>
</body>
</html>
